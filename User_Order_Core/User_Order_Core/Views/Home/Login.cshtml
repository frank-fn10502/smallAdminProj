@{
    ViewData["Title"] = "Login";
}


<div class="container" id="accountArea">
    <form>
        <div class="form-group row">
            <label for="staticEmail" class="col-sm-2 col-form-label">Account</label>
            <div class="col-sm-10">
                <input type="text" class="form-control" v-model="account">
            </div>
        </div>
        <div class="form-group row">
            <label for="inputPassword" class="col-sm-2 col-form-label">Password</label>
            <div class="col-sm-10">
                <input type="password" class="form-control" v-model="password">
            </div>
        </div>
        <button class="btn btn-primary" @@click.prevent="signIn">Sign in</button>
    </form>
</div>



@section TopCss
{
    <link href="~/css/Login.css" rel="stylesheet" />
}
@section BottomJS
{
    <script src="~/js/Login.js"></script>
}